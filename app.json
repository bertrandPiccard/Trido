{
  "expo": {
    "name": "Trido",
    "slug": "Trido",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/iconOfficial.png",
    "splash": {
      "image": "./app/assets/splashOfficial.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Trido offers a second life to your old or unused objects. With all the object around the house is hard to find a use for all of them, but instead of throwing them away, try Trido first!",
    "githubUrl": "https://github.com/bertrandPiccard/Trido"
  }
}
